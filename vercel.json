{
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11.0"
    }
  },
  "functions": {
    "src/api/index.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/src/api/index.py"
    }
  ]
}
